{
  "id": "login-module",
  "name": "login",
  "provides": [
    {
      "id": "login",
      "version": "0.1",
      "handlers" : [
        {
          "methods" : [ "POST" ],
          "pathPattern" : "/authn/login",
          "modulePermissions" : [ "auth.signtoken" ]
        },
        {
          "methods" : [ "GET" ],
          "pathPattern" : "/authn/credentials",
          "permissionsRequired" : [ "login.collection.get" ]
        },
        {
          "methods" : [ "GET" ],
          "pathPattern" : "/authn/credentials/{id}",
          "permissionsRequired" : [ "login.item.get" ]
        },
        {
          "methods" : [ "POST" ],
          "pathPattern" : "/authn/credentials",
          "permissionsRequired" : [ "login.item.post" ]
        },
        {
          "methods" : [ "PUT" ],
          "pathPattern" : "/authn/credentials",
          "permissionsRequired" : [ "login.item.put" ]
        },
        {
          "methods" : [ "DELETE" ],
          "pathPattern" : "/authn/credentials",
          "permissionsRequired" : [ "login.item.delete" ]
        }
      ]
    },
    {
      "id": "_tenant",
      "version": "1.0",
      "interfaceType" : "system",
      "handlers" : [
        {
          "methods" : [ "POST" ],
          "pathPattern" : "/_/tenant"
        }
      ]
    }
  ]
 }
