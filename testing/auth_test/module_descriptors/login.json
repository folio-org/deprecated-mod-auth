{
    "id" : "login-module",
    "name" : "login",
    "provides" : [
        {
            "id" : "login",
            "version" : "0.0.1"
        }
    ],
    "routingEntries" : [
        {
            "methods" : [ "POST" ],
            "path" : "/authn/login",
            "level" : "30",
            "type" : "request-response"
        },
	{
	    "methods" : [ "GET" ],
            "path" : "/authn/users",
            "level" : "30",
            "type" : "request-response",
            "permissionsRequired" : [ "login.readUser" ]
        },
        {
            "methods" : [ "POST" ],
            "path" : "/authn/users",
            "level" : "30",
            "type" : "request-response",
            "permissionsRequired" : [ "login.addUser" ]
        },
        {
            "methods" : [ "PUT" ],
            "path" : "/authn/users",
            "level" : "30",
            "type" : "request-response",
            "permissionsRequired" : [ "login.modifyUser" ]
        },
        {
            "methods" : [ "DELETE" ],
            "path" : "/authn/users",
            "level" : "30",
            "type" : "request-response",
            "permissionsRequired" : [ "login.removeUser" ]
        }
    ],
    "modulePermissions" : [ "auth.signtoken" ]
}
