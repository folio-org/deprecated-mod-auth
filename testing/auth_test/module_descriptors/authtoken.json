{
  "id": "authtoken-module",
  "name": "authtoken",
  "provides": [
    {
      "id": "authtoken",
      "version": "0.0.1",
      "handlers" : [
        {
          "methods" : [ "*" ],
          "pathPattern" : "/*",
          "level" : "10",
          "type" : "request-only",
          "modulePermissions" : [ "perms.users.read" ]
        },
        {
          "methods" : [ "POST" ],
          "pathPattern" : "/token",
          "level" : "20",
          "type" : "request-response"
        }
      ]
    }
  ]
}
